{
  "name": "node-package-template",
  "version": "0.0.1",
  "description": "Tamlate for creating a NodeJs package",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.mjs"
      }
    }
  },
  "scripts": {
    "clean": "pnpx rimraf ./.rollup.cache/ ./dist/ ./node_modules/ ./tsconfig.tsbuildinfo",
    "buildRollup": "pnpm exec rollup --config node:iiimaddiniii && pnpm dlx file-cli write ./dist/esm/package.json {\\\"type\\\":\\\"module\\\"}",
    "build": "pnpm install && pnpm run buildRollup",
    "buildCi": "set prod=true && pnpm run clean && pnpm install --frozen-lockfile && pnpm run buildRollup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IIIMADDINIII/node-package-template"
  },
  "author": "IIIMADDINIII <martin@elend.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IIIMADDINIII/node-package-template/issues"
  },
  "homepage": "https://github.com/IIIMADDINIII/node-package-template#readme",
  "devDependencies": {
    "@types/node": "^18.16.0",
    "typescript": "^5.0.4",
    "rollup": "^3.21.0",
    "rollup-config-iiimaddiniii": "^0.1.3"
  },
  "dependencies": {
    "tslib": "^2.5.0"
  },
  "rollup": {}
}